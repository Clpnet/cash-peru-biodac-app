<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AppProsegur" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/gray_custom</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/teal_700</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AppProsegur.NoActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AppProsegur.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AppProsegur.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppTheme" parent="Theme.MaterialComponents.Light.DarkActionBar"/>

    <style name="AppTheme.ActionBar" parent="AppTheme">
        <item name="android:layout_gravity">start</item>
    </style>
</resources>